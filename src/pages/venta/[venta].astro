---
import Layout from '../../layouts/Layout.astro'
import inmuebles from '../../data/inmuebles';


export async function getStaticPaths() {
    const inmueble = inmuebles;

    return inmueble.map((p) => {
        return{
            params: {venta: p.url},
            props: { p }
        }
    })
}


const {venta} = Astro.params
const { p } = Astro.props
---


<Layout title={venta}>
<div>
    <img src={p.imagen}/>
    <h1>{p.nombre}</h1>
</div>
</Layout>



